void main(){
  //while loop for degrees
  
  int mark =90;
  int add =6;
  while(mark <= 95 )
  {
    print("your grade is $mark you just need ${add-1} degree to get A+");
    mark++;
    add--;
  }
  
   //do-while loop for degrees
   do{
     print("your grade is $mark you just need ${add-1} degree to get A+");
     mark++;
     add--;
  }  while(mark <= 95 );
 
}
